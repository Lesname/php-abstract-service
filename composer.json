{
  "name": "lessname/abstract-service",
  "description": "Abstract service",
  "license": "proprietary",
  "repositories": [
    {
      "url": "https://gitlab.com/api/v4/group/6632935/-/packages/composer/packages.json",
      "type": "composer"
    }
  ],
  "require": {
    "php": "^8.2.0",

    "ext-openssl": "*",

    "psr/http-server-handler": "^1.0.1",

    "lessname/documentor": "^0.3.1",
    "lessname/resource": "^0.4.1",
    "lessname/domain": "^0.3.0",
    "lessname/validator": "^0.3.0",
    "lessname/queue": "^0.5.0",
    "lessname/http": "^0.4.1",
    "lessname/value-object": "^0.3.0",

    "lessname/abstract-client": "^0.3.0",

    "laminas/laminas-stratigility": "^3.8.0",
    "laminas/laminas-router": "^3.9.0",

    "mezzio/mezzio": "^3.11.0",
    "mezzio/mezzio-router": "^3.8.0",
    "mezzio/mezzio-helpers": "^5.8.0",
    "mezzio/mezzio-laminasrouter": "^3.4.0",

    "monolog/monolog": "^3.0.0",
    "sentry/sdk": "^4.0.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7.0",
    "phpunit/phpunit": "^10.1.0",
    "phpstan/phpstan": "^1.8.0",
    "vimeo/psalm": "^5.12.0",

    "lessname/token": "^0.2.0",

    "php-amqplib/php-amqplib": "^3.2.0"
  },
  "autoload": {
    "psr-4": {
      "LessAbstractService\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LessAbstractServiceTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
